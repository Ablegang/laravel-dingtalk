{
  "name": "chinaobject/laravel-dingtalk",
  "description": "This is a dingtalk extension of laravel",
  "keywords": ["laravel", "dingtalk", "chinaobject"],
  "license": "MIT",
  "authors": [
    {
      "name": "china object",
      "email": "lpg525@icloud.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "laravel/framework": "5.5.*",
    "chinaobject/laravel-pre-request":"dev-master"
  },
  "autoload": {
    "psr-4": {
      "Chinaobject\\Dingtalk\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Chinaobject\\Dingtalk\\DingtalkProvider"
      ],
      "aliases": {
        "Dingtalk": "Chinaobject\\Dingtalk\\Facade\\Dingtalk"
      }
    }
  },
  "minimum-stability": "dev",
  "require-dev": {
    "php": ">=7.0",
    "laravel/framework": "5.5.*"
  }
}
